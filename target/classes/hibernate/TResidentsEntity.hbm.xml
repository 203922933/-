<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="entity.TResidentsEntity" table="t_residents" schema="cpms">
        <id name="residentId" column="ResidentID"/>
        <property name="residentName" column="ResidentName"/>
        <property name="residentSex" column="ResidentSex"/>
        <property name="idNumber" column="IdNumber"/>
        <property name="phoneNumber" column="PhoneNumber"/>
        <property name="checkinTime" column="CheckinTime"/>
    </class>
</hibernate-mapping>